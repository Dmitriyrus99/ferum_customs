# Production environment example
SITE_NAME=erp.example.com
FRAPPE_ADMIN_PASSWORD=change_me
ADMIN_PASSWORD=change_me
DB_ROOT_PASSWORD=change_me
DB_ROOT_HOST=localhost

# TLS/SSL
USE_HTTPS=1
FRAPPE_URL=https://erp.example.com
# Enable HSTS and common security headers via your reverse proxy
ENABLE_SECURITY_HEADERS=1

# Redis password should be strong and different from ADMIN_PASSWORD
REDIS_PASSWORD=change_me_secure
REDIS_CACHE=redis://:${REDIS_PASSWORD}@redis-cache:6379/0
REDIS_QUEUE=redis://:${REDIS_PASSWORD}@redis-queue:6379/1
REDIS_SOCKETIO=redis://:${REDIS_PASSWORD}@redis-socketio:6379/2

